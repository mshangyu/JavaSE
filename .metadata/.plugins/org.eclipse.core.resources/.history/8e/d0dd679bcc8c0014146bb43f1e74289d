
package priority;

public class YieldDemo {
	
	public YieldDemo(String name){
		super(name);
	}
	
	public void run() {
		for (int i = 0; i < 100; i++) {
			System.out.println(getName()+"=="+i);
			//当i=55的时候 让当前县城给其它线程让步，自己步入线程池
			if (i==55) {
				this.yield();
			}
		}

	}
	public static void main(String[] args) {
		

	}

}
