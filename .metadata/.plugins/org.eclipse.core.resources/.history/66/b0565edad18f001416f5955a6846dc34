package redir_stdio;

import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.PrintStream;

/*System.in  键盘        setIn(InputSteam) 
 * System.out   屏幕  setOut(OutputSteam)
 * System.err   屏幕setError(PrintSteam)
 * */
public class RedirStdio {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		try {
			PrintStream ps=new PrintStream(new FileOutputStream("redir.txt"));
			//system.out--->ps
			System.setOut(ps);
			System.out.println("重定向之后打印到文件");
		
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		

	}

}
