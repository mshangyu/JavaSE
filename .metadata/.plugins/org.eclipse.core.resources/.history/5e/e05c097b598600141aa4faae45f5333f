package b_List;

import java.util.ArrayList;

import bean.Person;

public class ArrayListDemo2 {

	public static void main(String[] args) {
		ArrayList a1=new ArrayList();
		
		for(int i=0;i<5;i++){
			a1.add(new Person("it-"+i,i));
		}
		
		a1.add(new Person("it-3",3));
		System.out.println(a1);

	}

}
