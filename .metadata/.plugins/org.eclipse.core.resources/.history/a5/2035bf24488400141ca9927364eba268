class ArrayDemo{
	public static void main(String[] args){
		int[] arr=new int[3];
		System.out.println(arr);
		System.out.println(arr[2]);
		
		//需求：创建一个有三个int类型元素的数组
//注意数组的元素的（编号）索引的最大值是数组的长度减1
		int[] arr=new int[3];
		System.out.println(arr);
		System.out.println(arr[0]);
		System.out.println(arr[1]);
	}
}