package b_List;

import java.util.LinkedList;

public class LinkedListDemo {
	public static void main(String[] args) {
		LinkedList list=new LinkedList();
		//1\Ôö¼Ó
		list.add("hello");
		list.add("world");
		list.add("java");
		list.add("shujuku");
		
		list.add(2,"hellokimi");
		System.out.println(list);
	}

}
