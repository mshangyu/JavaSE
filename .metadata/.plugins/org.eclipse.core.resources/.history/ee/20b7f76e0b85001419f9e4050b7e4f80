
public class JiaMi {
	public static void main(String[] args) {
		int number=123456;
		
		if(number<0||number>99999999){
			System.out.println("Êı¾İÓĞÎó");
			return;
		}
		int[] arr=new int[8];
		int index=0;
		
		while(number>0){
			arr[index]=number%10;
			number/=10;
			index++;
			
		}
		for(int x=0;x<index;x++){
			System.out.println(arr[x]);
		}
		System.out.println();
	}

}
