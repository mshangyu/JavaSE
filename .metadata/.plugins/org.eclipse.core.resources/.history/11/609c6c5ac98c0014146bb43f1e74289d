package priority;

public class PriorityDemo {
	public static void main(String[] args) {
		Demo demo1=new Demo("线程1");
		Demo demo2=new Demo("线程2");
		Demo demo3=new Demo("线程3");
		
		demo1.start();
		demo2.start();
		demo3.start();
	}

}
class Demo extends Thread{
	String name;
	public Demo(){
		
	}
}