
public class Ticket2 implements Runnable {
	private static int tickets=10;
	@Override
	public void run() {
		if(tickets>0){
			System.out.println(Thread.currentThread().getName()+"正在出售第"+tickets+"张票");
		}

	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
