/*
 * 1、writer类中其他的write方法
 * 
 * 
 * */

package IO流;

import java.io.FileWriter;

public class FileWriteDemo1 {
	public static void main(String[] args) {
		//创建对象
		//如果文件不存在，就创建文件
		//如果文件存在，写数据的时候。就是数据的覆盖
		
		FileWriter fw=new FileWriter("fw3.txt");
		
		//write(String str)
		fw.write("hello");
		
		//释放资源
		fw.close();
	}

}
