}
interface SpeakEnglish{
	public abstract void speak();
}
abstract class Person{
	private String name;
	private int age;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public Person(){}
	public Person(String name,int age){
		this.name=name;
		this.age=age;
	}
	
	
}

abstract class  Lanqiu extends Person(){
	super(name,age);
}


