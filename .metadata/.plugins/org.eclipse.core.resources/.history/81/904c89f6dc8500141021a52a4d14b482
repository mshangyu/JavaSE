
public class Chang1 {
	public static void main(String[] args) {
		
	}

}

class Fang{
	private int width;
	private int length;
	
	public Fang(){}
	public Fang(int width,int length){
		this.width=width;
		this.length=length;
	}
	public void setWidth(int width){
		this.width=width;
	}
	public int getWidth(){
		return width;
	}
	public void setLength(int length){
		this.length=length;
	}
	public int getLength(){
		return length;
	}
}
